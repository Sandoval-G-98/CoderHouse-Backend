<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Products Home </title>
</head>
<body>
    {{#each productsList}}
    <div id="listProducts">
        <ul>
            <li>{{this.title}}</li>
            <li>{{this.description}}</li>
            <li>{{this.price}}</li>
            <li>{{this.thumbnails}}</li>
            <li>{{this.status}}</li>
            <li>{{this.code}}</li>
            <li>{{this.category}}</li>
            <li>{{this.stock}}</li>
        </ul>
    </div>
    {{/each}}
</body>
</html>